{
  "%selectors": {
    "hribbon": [
      0
    ],
    "p": [
      1
    ],
    "h1": [
      2
    ]
  },
  "rules": [
    {
      "media": "screen",
      "selectors": [
        "hribbon"
      ],
      "property": "-mkts-draggable",
      "value": "xy"
    },
    {
      "media": "*",
      "selectors": [
        "p"
      ],
      "property": "-mkts-columns",
      "value": "1"
    },
    {
      "media": "*",
      "selectors": [
        "h1"
      ],
      "property": "-mkts-columns",
      "value": "all"
    }
  ]
}
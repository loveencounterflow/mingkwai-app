{
  "%selectors": {
    "page h1": [
      0
    ],
    "ribbon.draggable": [
      1
    ]
  },
  "rules": [
    {
      "media": "*",
      "selectors": [
        "page h1"
      ],
      "property": "-mkts-columns",
      "value": "all"
    },
    {
      "media": "screen",
      "selectors": [
        "ribbon.draggable"
      ],
      "property": "-mkts-draggable",
      "value": "xy"
    }
  ]
}